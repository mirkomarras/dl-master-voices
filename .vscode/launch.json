{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Mastervoice",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/routines/mv/optimize.py",
            "console": "integratedTerminal",
            "args": [
                "--netv", "vggvox/v000",
                "--seed", "./data/vs_mv_seed/female/",
                "--audio_dir", "data/voxceleb2/dev",
                "--gender", "female",
                "--gradient", "pgd",
                "--learning_rate", "0.001",
                "--train_pop", "data/vs_mv_pairs/mv_train_population_small_100u_20s.csv",
                "--test_pop", "data/vs_mv_pairs/mv_test_population_small_100u_10s.csv",
                "--batch", "16",
                "--attack", "nes@wave",
                "--gm", "voxceleb/0/1024",
                "--n_epochs", "3",
                "--memory-growth"
            ],
            "env": {
                "CUDA_VISIBLE_DEVICES": "0",
                "TF_CPP_MIN_LOG_LEVEL": "3",
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "mv-rtvc-vctk",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/routines/mv/optimize.py",
            "console": "integratedTerminal",
            "args": [
                "--audio_meta", "data/vs_mv_pairs/meta_data_vctk.csv",
                "--netv", "vggvox/v000",
                "--seed", "data/vs_mv_seed/female/",
                "--audio_dir", "data/vctk",
                "--gender", "female",
                "--gradient", "pgd",
                "--learning_rate", "0.01",
                "--train_pop", "data/vs_mv_pairs/mv_train_population_vctk_50u_10s.csv",
                "--test_pop", "data/vs_mv_pairs/mv_test_population_vctk_50u_10s.csv",
                "--batch", "10",
                "--attack", "nes@cloning",
                "--gm", "voxceleb/0/1024",
                "--n_epochs", "3",
                "--memory-growth"
            ],
            "env": {
                "CUDA_VISIBLE_DEVICES": "0",
                "TF_CPP_MIN_LOG_LEVEL": "3",
                "PYTHONPATH": "${workspaceFolder}:${workspaceFolder}/rtvc"
            }
        },        
        {
            "name": "mv-rtvc-libri",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/routines/mv/optimize.py",
            "console": "integratedTerminal",
            "args": [
                "--audio_meta", "data/vs_mv_pairs/meta_data_librispeech.csv",
                "--netv", "vggvox/v000",
                "--seed", "data/vs_mv_seed/female/",
                "--audio_dir", "data/vctk",
                "--gender", "female",
                "--gradient", "pgd",
                "--learning_rate", "0.01",
                "--train_pop", "data/vs_mv_pairs/mv_train_population_libri_100u_10s.csv",
                "--test_pop", "data/vs_mv_pairs/mv_test_population_libri_100u_10s.csv",
                "--batch", "10",
                "--attack", "nes@cloning",
                "--gm", "voxceleb/0/1024",
                "--n_epochs", "3",
                "--memory-growth"
            ],
            "env": {
                "CUDA_VISIBLE_DEVICES": "0",
                "TF_CPP_MIN_LOG_LEVEL": "3",
                "PYTHONPATH": "${workspaceFolder}:${workspaceFolder}/rtvc"
            }
        },        
        {
            "name": "GM training",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/routines/gan/train.py",
            "console": "integratedTerminal",
            "args": [
                "-m", "vae", 
                "-d", "voxceleb", 
                "-z", "512",
                "-b", "128", 
                "-n", "32768", 
                "-e", "5000", 
                "--memory-growth"
            ],
            "env": {
                "CUDA_VISIBLE_DEVICES": "0",
                "TF_CPP_MIN_LOG_LEVEL": "3",
                "PYTHONPATH": "${workspaceFolder}"
            }
        }
    ]
}